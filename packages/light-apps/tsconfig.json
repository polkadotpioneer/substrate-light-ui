{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": "node_modules",
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
  "extends": "../../tsconfig"
}
